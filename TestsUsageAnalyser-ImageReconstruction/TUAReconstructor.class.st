Class {
	#name : #TUAReconstructor,
	#superclass : #Object,
	#instVars : [
		'moveActions'
	],
	#category : 'TestsUsageAnalyser-ImageReconstruction'
}

{ #category : #parsing }
TUAReconstructor >> parseChangeDictionnary: aChangeEventDictionnary [
	"aChangeEventDictionnary is generated by TestsUsageAnalyser"

	| actionType |
	actionType := aChangeEventDictionnary at: 'actionType'.
	TUAReconstructorActions
		subclassesDo: [ :aActionClass | 
			| anActionInstance |
			anActionInstance := aActionClass new.
			(anActionInstance isApplicableForAction: actionType)
				ifTrue: [ anActionInstance apply: aChangeEventDictionnary ] ]
]
